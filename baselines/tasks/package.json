{
  "name": "tasks",
  "version": "0.1.0",
  "description": "Tasks client for Node.js",
  "repository": "googleapis/nodejs-tasks",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "main": "build/src/index.js",
  "files": [
    "build/src",
    "build/protos"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google tasks",
    "tasks",
    "cloud tasks"
  ],
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r protos build/ && minifyProtoJson",
    "compile-protos": "compileProtos src",
    "docs": "jsdoc -c .jsdoc.js",
    "fix": "gts fix",
    "lint": "gts check",
    "prepare": "npm run compile-protos && npm run compile",
    "system-test": "c8 mocha build/system-test",
    "test": "c8 mocha build/test"
  },
  "dependencies": {
    "google-gax": "^5.0.0-rc.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.16.4",
    "@types/sinon": "^10.0.20",
    "c8": "^10.1.3",
    "gapic-tools": "^1.0.1",
    "gts": "5.3.1",
    "jsdoc": "^4.0.4",
    "jsdoc-fresh": "^3.0.0",
    "jsdoc-region-tag": "^3.0.0",
    "mocha": "^10.7.3",
    "pack-n-play": "^2.1.0",
    "sinon": "^18.0.0",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=v18"
  }
}
