{
  "repository": "googleapis/nodejs-showcase",
  "name": "showcase",
  "version": "0.1.0",
  "author": "Google LLC",
  "description": "GAPIC Showcase client for Node.js",
  "main": "build/src/index.js",
  "files": [
    "protos",
    "build/src",
    "!build/src/**/*.map",
    "AUTHORS",
    "COPYING"
  ],
  "dependencies": {
    "google-gax": "googleapis/gax-nodejs"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/through2": "^2.0.34",
    "gts": "^0.9.0",
    "mocha": "^6.0.0",
    "through2": "^3.0.0",
    "typescript": "~3.3.0"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "compile-protos": "pbjs -p protos -t static-module -o pbjs-genfiles/protos.js google/showcase/v1beta1/echo.proto && pbts pbjs-genfiles/protos.js -o pbjs-genfiles/protos.d.ts",
    "fix": "gts fix",
    "prepare": "npm run compile-protos && npm run compile",
    "pretest": "npm run compile-protos && npm run compile",
    "posttest": "npm run check"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  }
}
