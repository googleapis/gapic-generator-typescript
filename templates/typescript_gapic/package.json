{
  "name": "{{ api.publishName }}",
  "version": "0.1.0",
  "description": "{{ api.naming.productName }} client for Node.js",
  "repository": "googleapis/nodejs-{{ api.naming.productName.toKebabCase() }}",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "main": "build/src/index.js",
  "files": [
    "build/src",
    "build/protos"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google {{ api.naming.productName.toCamelCase() }}",
    "{{ api.naming.productName.toCamelCase() }}",
    "{{ api.uniqKeywords.join('\",\"') | safe }}"
  ],
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc -p . && cp -r protos build/",
    "compile-protos": "compileProtos src",
    "docs": "jsdoc -c .jsdoc.js",
    "predocs-test": "npm run docs",
    "docs-test": "linkinator docs",
    "fix": "gts fix",
    "lint": "gts check",
    "prepare": "npm run compile-protos && npm run compile",
    "system-test": "c8 mocha build/system-test",
    "test": "c8 mocha build/test"
  },
  "dependencies": {
    "google-gax": "^2.9.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "@types/sinon": "^9.0.8",
    "c8": "^7.3.4",
    "gts": "^3.0.1",
    "jsdoc": "^3.6.6",
    "jsdoc-fresh": "^1.0.2",
    "jsdoc-region-tag": "^1.0.6",
    "linkinator": "^2.2.0",
    "mocha": "^8.1.3",
    "null-loader": "^4.0.1",
    "pack-n-play": "^1.0.0-2",
    "sinon": "^9.2.0",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
