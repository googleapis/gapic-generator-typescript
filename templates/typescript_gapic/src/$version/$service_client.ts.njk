    });
{%- endif %}
    return this._innerApiCalls.{{ method.name.toCamelCase() }}(request, options, callback);
  } 
{%- endfor %}
{%- for method in service.paging %}
/**
  {{- util.printCommentsForMethod(method) }}
  {{- util.printRequest() -}}
