      callback?: Callback<
          protosTypes{{ util.toInterface(method.pagingResponseType) }}[],
          protosTypes{{ util.toInterface(method.inputInterface) }}|null,
          protosTypes{{ util.toInterface(method.outputInterface) }}>):
      Promise<[
        protosTypes{{ util.toInterface(method.pagingResponseType) }}[],
        protosTypes{{ util.toInterface(method.inputInterface) }}|null, 
        protosTypes{{ util.toInterface(method.outputInterface) }}
      ]>|void {
