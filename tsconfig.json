{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2018"],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2018",
    "rootDir": ".",
    "outDir": "build",
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "gapic_generator_typescript/*": [
        "*",
        "../../bazel-out/darwin-fastbuild/bin/external/gapic_generator_typescript/*",
        "../../bazel-out/k8-fastbuild/bin/external/gapic_generator_typescript/*",
        "../../bazel-out/x64_windows-fastbuild/bin/external/gapic_generator_typescript/*",
        "../../bazel-out/darwin-dbg/bin/external/gapic_generator_typescript/*",
        "../../bazel-out/k8-dbg/bin/external/gapic_generator_typescript/*",
        "../../bazel-out/x64_windows-dbg/bin/external/gapic_generator_typescript/*",
        "../../bazel-out/host/bin/external/gapic_generator_typescript/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
    "tools/**/*.ts",
    "./index.d.ts",
    "protos/index.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
